{
  "name": "compass-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Compass Personal Productivity System",
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ..",
    "build": "cd backend && npm run build && cd ../frontend && npm run build && cd ..",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:generate": "cd backend && npx prisma generate",
    "db:studio": "cd backend && npx prisma studio",
    "db:reset": "cd backend && npx prisma migrate reset",
    "setup": "bash scripts/setup.sh",
    "verify": "bash scripts/verify-environment.sh",
    "health": "bash scripts/health-check.sh",
    "check:anthropic": "CHECK_ANTHROPIC=true ./scripts/health-check.sh anthropic",
    "analyze": "npx ts-node analysis/orchestrator.ts",
    "analyze:parallel": "npx ts-node analysis/orchestrator.ts --parallel",
    "analyze:reliability": "npx ts-node analysis/agents/reliability-analyst.ts",
    "analyze:maintainability": "npx ts-node analysis/agents/maintainability-inspector.ts",
    "analyze:complexity": "npx ts-node analysis/agents/complexity-auditor.ts",
    "analyze:churn": "npx ts-node analysis/agents/churn-tracker.ts",
    "analyze:duplication": "npx ts-node analysis/agents/duplication-detective.ts",
    "analyze:coverage": "npx ts-node analysis/agents/coverage-analyst.ts",
    "analyze:debt": "npx ts-node analysis/agents/debt-accountant.ts",
    "analyze:sustainability": "npx ts-node analysis/agents/sustainability-evaluator.ts",
    "analyze:reviews": "npx ts-node analysis/agents/review-metrics-analyst.ts"
  },
  "keywords": ["productivity", "task-management", "calendar"],
  "author": "",
  "license": "MIT"
}
